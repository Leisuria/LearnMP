<!--components/w-swiper/index.wxml-->
<swiper class="swiper"
        circular
        autoplay
        interval="3000"
        duration="300"
        indicator-dots
        indicator-active-color="#9055A2">
  <block wx:for="{{list}}" wx:key="{{index}}">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
